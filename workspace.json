{
  "version": 2,
  "cli": { "defaultCollection": "@nrwl/react" },
  "defaultProject": "app-web",
  "generators": {
    "@nrwl/react": {
      "application": { "style": "less", "linter": "eslint", "babel": true },
      "component": { "style": "less" },
      "library": { "style": "less", "linter": "eslint" }
    }
  },
  "projects": {
    "app-web": {
      "root": "apps/app-web",
      "sourceRoot": "apps/app-web/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/app-web",
            "index": "apps/app-web/src/index.html",
            "main": "apps/app-web/src/main.tsx",
            "polyfills": "apps/app-web/src/polyfills.ts",
            "tsConfig": "apps/app-web/tsconfig.app.json",
            "assets": [
              "apps/app-web/src/favicon.ico",
              "apps/app-web/src/assets"
            ],
            "styles": ["apps/app-web/src/styles.less"],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/app-web/src/environments/environment.ts",
                  "with": "apps/app-web/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": { "buildTarget": "app-web:build" },
          "configurations": {
            "production": { "buildTarget": "app-web:build:production" }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/app-web/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/app-web"],
          "options": {
            "jestConfig": "apps/app-web/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "app-web-e2e": {
      "root": "apps/app-web-e2e",
      "sourceRoot": "apps/app-web-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/app-web-e2e/cypress.json",
            "tsConfig": "apps/app-web-e2e/tsconfig.e2e.json",
            "devServerTarget": "app-web:serve"
          },
          "configurations": {
            "production": { "devServerTarget": "app-web:serve:production" }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["apps/app-web-e2e/**/*.{js,ts}"] }
        }
      }
    },
    "auth": {
      "root": "libs/auth",
      "sourceRoot": "libs/auth/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
